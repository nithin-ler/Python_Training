   "metadata": {},
   "outputs": [],
   "source": [
    "def reverse(strg):\n",
    "    reved = \"\"\n",
    "    for i in range(len(strg)-1,-1,-1):\n",
    "        reved = reved + strg[i]\n",
    "    print(reved)"
   ]
