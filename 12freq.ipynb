   "id": "f166ca31-4acc-4c9a-863f-59611da962a5",
   "metadata": {},
   "outputs": [],
   "source": [
    "dct = {}\n",
    "lst = [4,4,7,6,2,4,5,7,8,6,5,4,3,3,5,6,5,2,5,8,7,4,5,8,7,5]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "58af373f-cc14-499c-ae0f-482e3a6ed52c",
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in lst:\n",
    "    if i not in dct:\n",
    "        dct[i]=1\n",
    "    else:\n",
    "        dct[i]=dct[i]+1"
   ]
  },
